{
  "env": {
    "browser": true,
    "es2021": true
  },
  // "extends": ["airbnb", "airbnb/hooks", "next", "next/core-web-vitals"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    // Customize your rules here
    "react/react-in-jsx-scope": "off",
    "react/jsx-props-no-spreading": "off"
  }
}
